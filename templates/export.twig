<table class="text-center" border="1">
    <thead class="text-center">
        <tr>
            <th>No</th>
            <th>Customer Code</th>
            <th>Customer Name</th>
            <th>Customer City</th>
            <th>Customer Country</th>
            <th>Agent Name</th>
            <th>Agent Work Area</th>
            <th>Phone Agent</th>
        </tr>
    </thead>
    <tbody class="text-center">
        {% for exp in data %}
        <tr>
        
            <td>{{loop.index}}</td>
            <td>{{exp.cust_code}}</td>
            <td>{{exp.cust_name}}</td>
            <td>{{exp.cust_city}}</td>
            <td>{{exp.cust_country}}</td>
            <td>{{exp.agent_name}}</td>
            <td>{{exp.working_agent}}</td>
            <td>{{exp.phone_agent}}</td>
        </tr>
        {% endfor %}
    </tbody>
</table>